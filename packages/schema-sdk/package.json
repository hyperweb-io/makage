{
  "name": "schema-sdk",
  "version": "0.15.1",
  "description": "Convert JSON Schema OpenAPI Spec to TypeScript Clients",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/hyperweb-io/dev-utils",
  "license": "MIT",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperweb-io/dev-utils"
  },
  "bugs": {
    "url": "https://github.com/hyperweb-io/dev-utils/issues"
  },
  "scripts": {
    "copy": "cpy ../../LICENSE README.md package.json dist --flat",
    "clean": "rimraf dist/**",
    "prepublishOnly": "npm run build",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/generator": "7.28.5",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "7.28.5",
    "@babel/types": "7.28.5",
    "@interweb/casing": "workspace:*",
    "@interweb/fetch-api-client": "workspace:*",
    "deepmerge": "^4.3.1",
    "fast-json-patch": "^3.1.1",
    "schema-typescript": "workspace:*"
  },
  "keywords": [
    "jsonschema",
    "schema",
    "typescript",
    "swagger",
    "openapi"
  ],
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "jest-diff": "^30.0.4"
  }
}
